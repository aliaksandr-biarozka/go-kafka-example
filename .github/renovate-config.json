{
    "branchPrefix": "test-renovate/",
    "rebaseWhen": "conflicted",
    "username": "renovate-release",
    "gitAuthor": "Renovate Bot <bot@renovateapp.com>",
    "onboarding": false,
    "platform": "github",
    "forkProcessing": "enabled",
    "repositories": [
      "aliaksandr-biarozka/go-kafka-example"
    ],
    "packageRules": [
      {
        "description": "Group all non-major into one PR",
        "extends": ["group:allNonMajor", "config:recommended", "helpers:pinGitHubActionDigests"],
        "dependencyDashboardApproval": false,
        "minimumReleaseAge": "14 days",
        "groupName": "non-major deps",
        "groupSlug": "non-major-deps"
      },
      {
        "description": "Group all major old updates into one PR",
        "matchCurrentAge": "> 13 months",
        "extends": ["config:recommended", "helpers:pinGitHubActionDigests"],
        "matchUpdateTypes": ["major"],
        "dependencyDashboardApproval": false,
        "groupName": "major-old",
        "groupSlug": "major-old"
      }
    ]
  }